{
  "name": "mernlearn",
  "version": "1.0.0",
  "description": "MERN learn",
  "main": "app.js",
  "scripts": {
    "starr": "cross-env NODE-ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install":"npm install --prefix client",
    "client:build":"npm run build --prefix client",
    "dev": "cross-env NODE-ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "mern",
    "react"
  ],
  "author": "Yura Paychek <email@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.2.1",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.7",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "cross-env": "^6.0.3"
  }
}
